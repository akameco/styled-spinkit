{
  "name": "styled-spinkit",
  "version": "1.1.0",
  "description": "Spinner Loading components",
  "license": "MIT",
  "repository": "akameco/styled-spinkit",
  "author": {
    "name": "akameco",
    "email": "akameco.t@gmail.com",
    "url": "https://akameco.github.io"
  },
  "main": "dist/index.js",
  "jsnext:main": "dist/styled-spinkit.esm.js",
  "module": "dist/styled-spinkit.esm.js",
  "sideEffects": false,
  "typings": "dist/index.d.ts",
  "keywords": [
    "react",
    "styled-components",
    "components",
    "Spinner",
    "Loading",
    "components"
  ],
  "scripts": {
    "prebuild": "rimraf dist/*",
    "build": "tsc && rollup -c",
    "postbuild": "rimraf compiled/*",
    "fmt": "prettier --write '**/*.{ts,tsx,js,json,md}'",
    "lint:css": "stylelint './src/**/*.{ts,tsx}'",
    "lint:js": "eslint --fix 'src/**/*.{ts,tsx}'",
    "lint": "npm run lint:css && npm run lint:js",
    "test": "jest",
    "dev": "parcel example/index.html",
    "prepublish": "npm run build"
  },
  "lint-staged": {
    ".{ts,tsx,js,json,md}": [
      "prettier --write"
    ]
  },
  "files": [
    "dist"
  ],
  "dependencies": {},
  "devDependencies": {
    "@akameco/tsconfig": "0.4.0",
    "@types/jest": "26.0.23",
    "@types/react": "17.0.11",
    "@types/react-test-renderer": "17.0.1",
    "@types/styled-components": "5.1.0",
    "eslint": "6.8.0",
    "eslint-config-precure": "5.4.0",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "jest-styled-components": "7.0.2",
    "lint-staged": "10.1.6",
    "parcel-bundler": "1.12.4",
    "prettier": "2.0.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-test-renderer": "16.13.1",
    "rimraf": "3.0.2",
    "rollup": "2.6.1",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "5.3.0",
    "storyshots": "3.2.2",
    "styled-components": "5.1.0",
    "stylelint": "13.3.2",
    "stylelint-config-standard": "20.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "ts-jest": "25.4.0",
    "typescript": "3.8.3"
  },
  "peerDependencies": {
    "react": ">=15.0.0",
    "styled-components": ">=5.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
