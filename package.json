{
  "name": "styled-spinkit",
  "version": "0.5.1",
  "description": "Spinner Loading components",
  "license": "MIT",
  "repository": "akameco/styled-spinkit",
  "author": {
    "name": "akameco",
    "email": "akameco.t@gmail.com",
    "url": "https://akameco.github.io"
  },
  "main": "lib/index.js",
  "jsnext:main": "dist/styled-spinkit.es.js",
  "module": "dist/styled-spinkit.es.js",
  "keywords": [
    "react",
    "styled-components",
    "components",
    "flow",
    "flowtype",
    "Spinner",
    "Loading",
    "components"
  ],
  "scripts": {
    "add-contributors": "all-contributors add",
    "prebuild:lib": "rimraf lib/*",
    "build": "npm run build:lib && npm run build:dist && npm run build:flow",
    "build:lib": "babel src/ -d lib/ --ignore '**/*.test.js'",
    "prebuild:dist": "rimraf dist/*",
    "build:dist": "rollup -c",
    "build:flow": "flow-copy-source -v -i '{**/tests/*.js,**/*.test.js}' src lib",
    "fmt": "prettier --write '**/*.{js,json,md}'",
    "storybook": "start-storybook -p 9009",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "lint:css": "stylelint \"./src/**/*.js\" || true",
    "lint:js": "eslint --fix src",
    "lint": "npm run lint:css && npm run lint:js",
    "test": "jest",
    "prepublish": "npm run build"
  },
  "lint-staged": {
    ".{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "flow-typed",
    "lib",
    "dist",
    "src"
  ],
  "dependencies": {
    "prop-types": "^15.6.2",
    "round-to": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/preset-flow": "7.0.0",
    "@kadira/storybook-deployer": "1.2.0",
    "@storybook/addon-actions": "4.1.11",
    "@storybook/addon-info": "4.1.11",
    "@storybook/addon-knobs": "4.1.11",
    "@storybook/addon-storyshots": "4.1.11",
    "@storybook/react": "4.1.11",
    "all-contributors-cli": "5.11.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.0.0",
    "babel-loader": "8.0.5",
    "babel-plugin-flow-react-proptypes": "25.0.0",
    "babel-plugin-styled-components": "1.10.0",
    "babel-preset-zero": "0.7.0",
    "eslint": "5.13.0",
    "eslint-config-precure": "3.5.0",
    "flow-bin": "0.92.0",
    "flow-copy-source": "2.0.2",
    "husky": "1.3.1",
    "jest": "24.0.0",
    "jest-styled-components": "6.3.1",
    "lint-staged": "8.1.2",
    "prettier": "1.16.3",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-test-renderer": "16.7.0",
    "rimraf": "2.6.3",
    "rollup": "1.1.2",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-flow": "1.1.1",
    "rollup-plugin-node-resolve": "4.0.0",
    "storyshots": "3.2.2",
    "styled-components": "4.1.3",
    "stylelint": "9.10.1",
    "stylelint-config-standard": "18.2.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.5.2"
  },
  "peerDependencies": {
    "react": ">=15.0.0",
    "styled-components": ">=2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
