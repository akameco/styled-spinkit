{
  "name": "styled-spinkit",
  "version": "0.7.0",
  "description": "Spinner Loading components",
  "license": "MIT",
  "repository": "akameco/styled-spinkit",
  "author": {
    "name": "akameco",
    "email": "akameco.t@gmail.com",
    "url": "https://akameco.github.io"
  },
  "main": "dist/index.js",
  "jsnext:main": "dist/styled-spinkit.esm.js",
  "module": "dist/styled-spinkit.esm.js",
  "sideEffects": false,
  "typings": "dist/index.d.ts",
  "keywords": [
    "react",
    "styled-components",
    "components",
    "Spinner",
    "Loading",
    "components"
  ],
  "scripts": {
    "add-contributors": "all-contributors add",
    "build": "tsc && rollup -c",
    "prebuild": "rimraf dist/*",
    "fmt": "prettier --write '**/*.{ts,tsx,js,json,md}'",
    "lint:css": "stylelint \"./src/**/*.js\" || true",
    "lint:js": "eslint --fix 'src/**/*.{ts,tsx}'",
    "lint": "npm run lint:css && npm run lint:js",
    "test": "jest",
    "prepublish": "npm run build"
  },
  "lint-staged": {
    ".{ts,tsx,js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "round-to": "^3.0.0"
  },
  "devDependencies": {
    "@akameco/tsconfig": "^0.1.1",
    "@types/jest": "^24.0.6",
    "@types/react": "16.8.4",
    "@types/react-test-renderer": "^16.8.1",
    "@types/round-to": "^3.0.0",
    "@types/styled-components": "^4.1.10",
    "all-contributors-cli": "6.1.2",
    "eslint": "5.14.1",
    "eslint-config-precure": "4.8.0",
    "husky": "1.3.1",
    "jest": "24.1.0",
    "jest-styled-components": "6.3.1",
    "lint-staged": "8.1.4",
    "prettier": "1.16.4",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-test-renderer": "16.8.3",
    "rimraf": "2.6.3",
    "rollup": "1.2.2",
    "rollup-plugin-node-resolve": "4.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "storyshots": "3.2.2",
    "styled-components": "4.1.3",
    "stylelint": "9.10.1",
    "stylelint-config-standard": "18.2.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.5.2",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.3333"
  },
  "peerDependencies": {
    "react": ">=15.0.0",
    "styled-components": ">=2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
